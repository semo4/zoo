{
    "version": 2,
    "name": "Zoo-Me",
    "builds": [
        {
            "src": "Zoo_Me/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "30mb"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Zoo_Me/wsgi.py"
        }
    ]
}